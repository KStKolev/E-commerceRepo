<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>aspnet-E_commerceApplication-0edefbca-d5b4-4685-86b5-ab33c21b1dd3</UserSecretsId>
    <RootNamespace>E_commerceApplication</RootNamespace>
